  <!-- Autocomplete buttons -->
  <Button name="LootLinkAutoCompleteButtonTemplate" virtual="true">
    <Size x="1920" y="16"/>
    <Scripts>
      <OnClick function="LootLink_AutoCompleteButton_OnClick"/>
      <OnEnter>
        LootLinkAutoCompleteButton_OnEnter(self);
      </OnEnter>
      <OnUpdate>
        if( GameTooltip:IsOwned(self) ) then
          LootLinkAutoCompleteButton_OnEnter(self);
        end
      </OnUpdate>
      <OnLeave>
        LootLinkAutoCompleteButton_OnLeave(self);
      </OnLeave>
      <OnLoad>
        self:GetFontString():SetPoint("LEFT", self, "LEFT", 15, 1);
      </OnLoad>
    </Scripts>
    <ButtonText name="$parentText"/>
    <NormalFont style="GameFontNormal"/>
    <HighlightFont style="GameFontHighlight"/>
    <DisabledFont style="GameFontDisable"/>
